<?xml version="1.0" encoding="UTF-8"?>
<applicationOptions>
        <friendlyName>BeyondJ Performance Test Web App</friendlyName>
        <deploymentType>SERVLET_CONTAINER</deploymentType>
        <war>beyondj-service</war>
        <requiresWar>true</requiresWar>
        <systemGateway>false</systemGateway>
        <warLocation>beyondj-service</warLocation>
        <contextPath>/beyondj-service</contextPath>
        <port>9040</port>
        <bundleId>12dc8404-a8b7-4a79-a02d-31a715359f60</bundleId>
        <gracefulStopTimeInMillis>3000</gracefulStopTimeInMillis>
        <version>1.0</version>
        <failureActionOption>REMOVE</failureActionOption>
        <sessionTimeout>30</sessionTimeout>
        <registerDefaultServlet>true</registerDefaultServlet>
        <registerJspServlet>true</registerJspServlet>
        <!--Possible values are JETTY, TOMCAT -->
        <containerType>JETTY</containerType>
        <printClassPath>true</printClassPath>
        <numInstances>3</numInstances>
        <loadBalancerDefType>ROUND_ROBIN_LOAD_BALANCER</loadBalancerDefType>
        <enableHealthCheck>true</enableHealthCheck>
        <healthCheckInterval>1</healthCheckInterval>
        <healthCheckInitialDelay>1</healthCheckInitialDelay>
        <!--Possible values: DAYS ,HOURS, MICROSECONDS ,MILLISECONDS ,MINUTES ,NANOSECONDS ,SECONDS -->
        <healthCheckIntervalUnit>MINUTES</healthCheckIntervalUnit>
        <healthCheckInitialDelayUnit>MINUTES</healthCheckInitialDelayUnit>
        <deployedPostContainerStart>false</deployedPostContainerStart>
        <enableJolokia>false</enableJolokia>
        <enableMetrics>false</enableMetrics>
        <metricsCollectionInterval>15000</metricsCollectionInterval>
        <enableJmxConnector>false</enableJmxConnector>
        <printHealthCheckNotice>false</printHealthCheckNotice>
        <requireAllPorts>false</requireAllPorts>
        <iconBase64>data:image/png;base64,</iconBase64>
</applicationOptions>
